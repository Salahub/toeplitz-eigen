myDist <- function(m, rho) -8*rho^m + 2*((1 - rho^(2*m))/(1 - rho^2) + 6*rho^m)/m - 4*((rho^2*(1 - rho^(2*m)))/(1 - rho^2)^2 - rho^m)/m^2 + 4*rho^2*((2*rho^2*(1 - rho^(2*m)))/(1 - rho^2)^3 + (rho^2*(1 - rho^(2*m)))/(1 - rho^2)^2)/m^2
gsDist <- function(m, rho) 4*rho^(2*m)/(1 - rho^m)^2 + (2/(1 - rho^m)^2)*((1 - rho^(4*m))/(1 - rho^2) - 2*rho^(2*m))/m
curve(myDist)
plot(xlab = "M", ylab = "Squared distance", x = seq(1, 200, by = 1), y = myDist(seq(1, 200, by = 1), 0.5))
plot(xlab = "M", ylab = "Squared distance", x = seq(1, 200, by = 1), y = myDist(seq(1, 200, by = 1), 0.5), type = 'l', col = "steelblue")
plot(xlab = "M", ylab = "Squared distance", x = seq(2, 200, by = 1), y = myDist(seq(1, 200, by = 1), 0.5), type = 'l', col = "steelblue")
plot(xlab = "M", ylab = "Squared distance", x = seq(2, 200, by = 1), y = myDist(seq(2, 200, by = 1), 0.5), type = 'l', col = "steelblue")
lines(x = seq(2, 200, by = 1), y = gsDist(seq(2, 200, by = 1), 0.5), col = "firebrick")
