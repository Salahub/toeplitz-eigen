myDist <- function(m, rho = 0.5) 2/(m*(1-rho^2)) - 4*rho^2/(m^2*(1 - rho^2)^2) + 4*rho^4/(m^3*(1 - rho^2)^3) - 4*rho^6/(m^3*(1 - rho^2)^3) + (-8 + 12/m - 4/m^2)*rho^m - 2/(m*(1 - rho^2))*rho^(2*m) + 4*rho^(2*(m+1))/(m^2*(1 - rho^2)^2) - 12/(m^3*(1 - rho^2)^3)*rho^(2 * (m + 2)) + 4*rho^(2 * (m+3))/(m^3*(1 - rho^2)^3)
gsDist <- function(m, rho = 0.5) 2/(m*(1-rho^2)*(1 - rho^m)^2) + (4/(1 - rho^m)^2 - 4/(M*(1-rho^m)^2))*rho^(2*m) - 2/(m*(1 - rho^m)^2*(1 - rho^2))*rho^(4*m)
plot(xlab = "M", ylab = "Distance", x = seq(2, 200, by = 1), y = log(gsDist(seq(2, 200, by = 1), 0.7)), type = 'l', col = "steelblue")
